{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:promise/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "promise"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "rules": {
      "prettier/prettier": "error"
    }
  }
}